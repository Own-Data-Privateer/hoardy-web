{
    "icons": {
        "48": "icon/main.svg",
        "96": "icon/main.svg",
        "128": "icon/main.svg"
    },

    "browser_action": {
        "default_icon": {
            "48": "icon/main.svg",
            "96": "icon/main.svg",
            "128": "icon/main.svg"
        }
    },

    "permissions": [
        "<all_urls>",
        "menus",
        "notifications",
        "storage",
        "tabs",
        "webRequest",
        "webRequestBlocking"
    ],

    "browser_specific_settings": {
        "gecko": {
            "id": "pwebarc@oxij.org",
            "strict_min_version": "68.0"
        }
    },

    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+A"
            }
        },
        "show-state": {
            "suggested_key": {
                "default": "Alt+G"
            }
        },
        "show-log": {
            "suggested_key": {
                "default": "Alt+Shift+G"
            }
        },
        "show-tab-log": {
            "suggested_key": {
                "default": "Alt+Shift+I"
            }
        },
        "toggle-tabconfig-children-tracking": {
            "suggested_key": {
                "default": "Alt+Shift+C"
            }
        },
        "toggle-tabconfig-children-limbo": {
            "suggested_key": {
                "default": "Alt+Shift+L"
            }
        },
        "collect-all-tab-inlimbo": {
            "suggested_key": {
                "default": "Alt+Shift+A"
            }
        },
        "discard-all-tab-inlimbo": {
            "suggested_key": {
                "default": "Alt+Shift+D"
            }
        }
    }
}
