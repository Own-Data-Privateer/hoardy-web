{
    "manifest_version": 2,

    "icons": {
        "128": "icon/128/on.png"
    },

    "browser_action": {
        "default_title": "pWebArc",
        "default_popup": "page/popup.html",
        "default_icon": {
            "128": "icon/128/on.png"
        }
    },

    "background": {
        "page": "background/main.html"
    },

    "permissions": [
        "debugger",
        "storage",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}
